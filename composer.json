    "keywords": ["javascript", "jquery", "file_upload"],
    "homepage": "https://mouf-php.com/packages/mouf/javascript.jquery.file-upload",
    "type": "mouf-library",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com",
            "homepage": "http://mouf-php.com"
        }
    ],
    "require": {
    	"php": ">=5.3.0",
    	"blueimp/jquery-file-upload": "*"
    },
    "autoload": {
        "psr-4": {
            "Mouf\\Javascript\\Jquery\\FileUpload\\": "src/"
